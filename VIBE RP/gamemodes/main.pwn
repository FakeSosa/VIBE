		IzmenaFirme[playerid] = -1;
	}
	else if(!strcmp(akcija, "proizvodi", true))
	{
        FirmaInfo[IzmenaFirme[playerid]][fProdukti] = radnja;
		FirmaLabelIPickup(i);

