	mysql_tqueryEx(SQL, "UPDATE `users` SET `Password` = '%e' WHERE `SQLID` = '%d'", pass, PI[playerid][SQLID]);

	va_SendClientMessage(playerid, 0x1b71bcFF,"--------------------------------------------------------------");
