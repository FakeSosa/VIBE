    else if( odigrao == 4 && rezultat >= 3) gorivostring = "Pogodak";//3+
    else if( odigrao == 4) gorivostring = "Promasaj";//3+
    else if( odigrao == 5 && rezultat >= 4 ) gorivostring = "Pogodak";
