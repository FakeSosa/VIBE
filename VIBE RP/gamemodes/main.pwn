			else if(PI[playerid][Clan] == 5) boja = "DABB3E"; //MS13
			else if(PI[playerid][Clan] == 6) boja = "FF0000"; //BDT
			else if(PI[playerid][Clan] == 7) boja = "656463"; //LCN
