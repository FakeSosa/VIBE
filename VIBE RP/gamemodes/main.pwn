		IzmenaFirme[playerid] = -1;
	}
	else if(!strcmp(akcija, "kasa", true))
	{
        FirmaInfo[IzmenaFirme[playerid]][fKasa] = radnja;
		FirmaLabelIPickup(i);

		mysql_tqueryEx(SQL, "UPDATE `business` SET `Kasa` = '%d' WHERE `SQLID` = '%d'",
