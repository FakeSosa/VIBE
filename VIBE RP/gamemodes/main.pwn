    else if( rezultat == 6 ) gorivostring = "Pogodak";//gg&3+
    else if( rezultat == 7 ) gorivostring = "Promasaj";//gg&3+
    //else if( rezultat == 0 ) gorivostring = "Ceka se";
    else gorivostring = "Unknown";
    return gorivostring;
}

