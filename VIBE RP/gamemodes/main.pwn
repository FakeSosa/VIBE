	}
	else if(orgid > 0)
	{
	    if(PI[id][Lider] != 0)
			return va_SendClientMessage(playerid, 0xFF0000FF, "#Greska: {FFFFFF}Taj igrac je vec lidere neke organizacije, prvo mu skinite lidera!");

		if(orgid == PI[id][Lider])
